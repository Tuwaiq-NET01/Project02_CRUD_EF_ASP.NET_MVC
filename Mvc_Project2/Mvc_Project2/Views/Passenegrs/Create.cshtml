@model PassengerModel

<form asp-action="Create" method="post" class="form-horizontal">

    <div class="fornm-group input-group mb-3">
        <div class="input-group-prepend">
            <label asp-for="Id" class="input-group-text" >Id </label>
            <input asp-for="Id"  class="form-control"aria-label="Username" aria-describedby="basic-addon1"/>
        </div>

    </div>

    <div class="fornm-group input-group mb-3">

        <label asp-for="Name" class="input-group-text">Name </label>
        <input asp-for="Name" class="form-control"aria-label="Username" aria-describedby="basic-addon1"/>
    </div>

    <div class="input-group-prepend mb-3">
        <label asp-for="Email" class="input-group-text">Email</label>
        <input asp-for="Email" class="form-control"aria-label="Username" aria-describedby="basic-addon1" /> <span class="input-group-text" id="basic-addon2">@@example.com</span>
    </div>

    <div class="fornm-group input-group mb-3">

        <label asp-for="PhoneNum" class="input-group-text">Phone Number</label>
        <input asp-for="PhoneNum" class="form-control"aria-label="Username" aria-describedby="basic-addon1"/>
    </div>

    <div class="fornm-group input-group mb-3">

        <label asp-for="PlaneId" class="input-group-text">PlaneId</label>
        <input asp-for="PlaneId" class="form-control"aria-label="Username" aria-describedby="basic-addon1"/>
    </div>

    <div class="fornm-group">
        <input type="submit" class="btn btn-primary" value="submit" />
    </div>

</form>
</div>