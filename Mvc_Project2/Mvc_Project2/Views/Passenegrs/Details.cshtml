@{
    var passenegrs = (PassengerModel)ViewData["Passenegrs"];
}


<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Phone Number</th>
            <th scope="col" style="color:red;">PlaneId</th>
        </tr>
    </thead>
    <tbody>

        <tr>
            <th scope="row">@passenegrs.Id</th>
            <td>@passenegrs.Name</td>
            <td>@passenegrs.Email</td>
            <td>@passenegrs.PhoneNum</td>
            <td style="color:red;">@passenegrs.PlaneId</td>
        </tr>
    </tbody>
</table>

